{
  "name": "typeless-starter",
  "version": "0.1.0",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "_lint": "eslint --ext js,jsx,ts,tsx .",
    "lint": "yarn _lint",
    "lint-fix": "yarn _lint --fix",
    "_prettier": "prettier \"src/**/*.{ts,tsx,json,css,scss}\"",
    "format": "yarn _prettier --write",
    "format-check": "yarn _prettier --list-different",
    "check-style": "yarn format-check && yarn lint",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/storybook__addon-links": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "@types/webpack-env": "^1.15.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "navi": "^0.14.0",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "react-navi": "^0.14.3",
    "rxjs": "^6.5.4",
    "typeless": "^1.3.0",
    "typescript": "^3.7.5"
  },
  "license": "MIT"
}
