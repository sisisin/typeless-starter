{
  "name": "typeless-starter",
  "version": "0.1.0",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "run-s verify test-*",
    "test-cra": "react-scripts test",
    "debug:test-cra": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "verify": "run-p verify-*",
    "verify-lint": "yarn _lint",
    "verify-type": "tsc",
    "verify-format": "yarn _prettier --list-different",
    "_lint": "eslint --ext js,jsx,ts,tsx .",
    "_prettier": "prettier \"**/*.{json,css,scss}\"",
    "fix": "run-p fix-*",
    "fix-lint": "yarn _lint --fix",
    "fix-format": "yarn _prettier --write",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "gf": "plop feature",
    "gm": "plop module"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@loadable/component": "^5.12.0",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.1",
    "@storybook/react": "^5.3.19",
    "@types/jest": "^26.0.0",
    "@types/loadable__component": "^5.10.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/storybook__addon-links": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "@types/webpack-env": "^1.15.2",
    "babel-loader": "^8.1.0",
    "commandpost": "^1.4.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.1",
    "prettier": "^2.0.5",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "rxjs": "^6.5.5",
    "ts-morph": "^7.1.1",
    "ts-node": "^8.10.2",
    "typeless": "^1.6.0",
    "typescript": "^3.9.5"
  },
  "license": "MIT"
}
