{
  "name": "typeless-starter",
  "version": "0.1.0",
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "^3.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "verify": "run-p verify-*",
    "verify-lint": "yarn _lint",
    "verify-type": "tsc",
    "verify-format": "yarn _prettier --list-different",
    "_lint": "eslint --ext js,jsx,ts,tsx .",
    "_prettier": "prettier \"**/*.{json,css,scss}\"",
    "fix": "run-p fix-*",
    "fix-lint": "yarn _lint --fix",
    "fix-format": "yarn _prettier --write",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "gf": "plop feature",
    "gm": "plop module"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@loadable/component": "^5.12.0",
